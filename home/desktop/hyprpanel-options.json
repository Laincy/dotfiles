{
  "theme.bar.floating": false,
  "theme.bar.layer": "top",
  "bar.launcher.autoDetectIcon": true,
  "bar.customModules.updates.pollingInterval": 1440000,
  "terminal": "alacritty",
  "bar.layouts": {
    "0": {
      "left": [
        "dashboard",
        "workspaces",
        "windowtitle"
      ],
      "middle": [
        "media"
      ],
      "right": [
        "volume",
        "network",
        "bluetooth",
        "battery",
        "systray",
        "clock",
        "notifications"
      ]
    }
  },
  "bar.scrollSpeed": 0,
  "bar.workspaces.show_numbered": true,
  "bar.clock.format": "%a %b %d  %I:%M %p"
}